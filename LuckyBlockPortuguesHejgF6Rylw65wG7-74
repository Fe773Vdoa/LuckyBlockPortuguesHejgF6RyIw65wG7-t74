local Tab = Window:MakeTab({
	Name = "üìç‚îÉTeleport",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "üü£ ‚Ä¢ Time Roxo",
	Callback = function()
      		local destinationPartId = "-925.9014892578125, 193.5000457763672, 286.02874755859375"

local function teleportToDestination(player)
    local character = player.Character
    if character then
        local destinationPart = game.Workspace:FindFirstChild(destinationPartId)
        if destinationPart then
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
            if humanoidRootPart then
                humanoidRootPart.CFrame = destinationPart.CFrame
            end
        end
    end
end

local button = script.Parent

button.MouseButton1Click:Connect(function()
    teleportToDestination(game.Players.LocalPlayer)
end)
  	end    
})

